{
    "name": "quendoo/ai-dashboard-backend",
    "description": "PHP backend proxy for MCP servers",
    "type": "project",
    "require": {
        "php": ">=8.1",
        "slim/slim": "^4.13",
        "slim/psr7": "^1.6",
        "guzzlehttp/guzzle": "^7.8",
        "monolog/monolog": "^3.5",
        "vlucas/phpdotenv": "^5.6",
        "ramsey/uuid": "^4.7"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    }
}
